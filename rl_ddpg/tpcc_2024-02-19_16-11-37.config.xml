<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
  <reconnectOnConnectionFailure>true</reconnectOnConnectionFailure>
  <isolation>TRANSACTION_SERIALIZABLE</isolation>
  <batchsize>128</batchsize>
  <scalefactor>1</scalefactor>
  <terminals>1</terminals>
  <works>
    <work>
      <time>60</time>
      <rate>10000</rate>
      <weights>45,43,4,4,4</weights>
    </work>
  </works>
  <transactiontypes>
    <transactiontype>
      <name>NewOrder</name>
    </transactiontype>
    <transactiontype>
      <name>Payment</name>
    </transactiontype>
    <transactiontype>
      <name>OrderStatus</name>
    </transactiontype>
    <transactiontype>
      <name>Delivery</name>
    </transactiontype>
    <transactiontype>
      <name>StockLevel</name>
    </transactiontype>
  </transactiontypes>
</configuration>
